{
    "extends": ["airbnb", "prettier"],
    "plugins": ["jest"],
    "rules": {
        "no-underscore-dangle": "off",
        "react/jsx-indent": 4,
        "react/jsx-indent-props": 4,
        "react/jsx-filename-extension": "off",
        "no-console": "off",
        "react/destructuring-assignment": "off"
    },
    "env": {
        "jest/globals": true
    }
}
